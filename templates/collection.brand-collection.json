    "rich_text_JdnBGe": {
      "type": "rich-text",
      "blocks": {
        "heading_fifxVA": {
          "type": "heading",
          "settings": {
            "heading": "Custom Corporate Gifts, Brand Activations & Event Launch Cocktails",
